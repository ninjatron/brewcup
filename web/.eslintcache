rmData();\n      formData.append('avatar', updatedAvatar[0], updatedAvatar[0].name);\n      UserService.updateAvatar(user._id, formData);\n    }\n    if (userUpdated) {\n      console.log(user);\n      UserService.update(user)\n        .then(updatedUser => {\n          setUser(updatedUser);\n        })\n        .catch(err => {\n          console.log(\"show some feedback later\");\n        })\n    }\n  }\n\n  useEffect(() => {\n    getUserData();\n  }, [user._id]);  \n\n  return (\n    <MyAccountWrapper>\n      <h3>My Account</h3>\n      <ProfileFormWrapper>\n        <DropzoneAreaWrapper avatarUrl={user.avatarUrl}>\n          <DropzoneArea\n              Icon={''}\n              acceptedFiles={['image/*']}\n              dropzoneText={\"\"}\n              onChange={handleAvatarChange}\n              filesLimit={1}\n              maxFileSize={1024000}\n          />\n          <p>Max 1mb file size.</p>\n        </DropzoneAreaWrapper>\n        <ProfileForm>\n          <TextField\n            label=\"Full Name\"\n            value={user.fullname}\n            name=\"fullname\"\n            onChange={handleChange}\n            variant=\"outlined\"\n          />\n          <TextField\n            label=\"E-mail\"\n            value={user.email}\n            name=\"email\"\n            onChange={handleChange}\n            variant=\"outlined\"\n          />\n          <TextField\n            label=\"Location\"\n            value={user.location}\n            name=\"location\"\n            onChange={handleChange}\n            variant=\"outlined\"\n          />\n          <SubmitButton onClick={saveChanges}>Update Profile</SubmitButton>\n        </ProfileForm>\n      </ProfileFormWrapper>\n    </MyAccountWrapper>\n  )\n};\n\nexport default MyAccount;","/home/aysad/Desktop/BrewAndCup/web/src/index.js",[],"/home/aysad/Desktop/BrewAndCup/web/src/containers/UserProfile.js",["146","147","148","149"],"import React, { Component, Fragment } from 'react';\nimport styled, { createGlobalS